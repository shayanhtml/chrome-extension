{
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "Logger.js",
        "content-script.js"
      ],
      "matches": [
        "https://przegladarka-ekw.ms.gov.pl/eukw_prz/KsiegiWieczyste/wyszukiwanieKW*"
      ],
      "run_at": "document_end"
    },
    {
      "all_frames": true,
      "js": [
        "cts.js"
      ],
      "matches": [
        "https://przegladarka-ekw.ms.gov.pl/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "description": "Rozszerzenie pomaga w pobieraniu zawartości ksiąg wieczystych na dysk.",
  "externally_connectable": {
    "matches": [
      "*://*.przegladarka-ekw.ms.gov.pl/*",
      "*://*.google.com/recaptcha/api2/anchor*"
    ]
  },
  "host_permissions": [
    "*://*.przegladarka-ekw.ms.gov.pl/*",
    "*://.przegladarka-ekw.ms.gov.pl/*",
    "https://engageub.pythonanywhere.com/*"
  ],
  "icons": {
    "128": "icon.png"
  },
  "key": "",
  "manifest_version": 3,
  "name": "Księgi wieczyste pobieracz",
  "permissions": [
    "scripting",
    "declarativeNetRequest",
    "storage",
    "proxy",
    "cookies",
    "tabs",
    "browsingData",
    "webRequest",
    "webRequestAuthProvider"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.0",
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "cts.js",
        "utils.js"
      ]
    }
  ]
}